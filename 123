@Test
void testStringClassButNotValueString() throws Exception {
    ObjectMapper mapper = new ObjectMapper();
    JavaType type = TypeFactory.defaultInstance().constructType(String.class);
    IgnoreStringDeserializer d = new IgnoreStringDeserializer(mock(JsonDeserializer.class), type);

    JsonParser p = mapper.getFactory().createParser("123"); // number, not quoted
    p.nextToken(); // VALUE_NUMBER_INT
    Object result = d.deserialize(p, mapper.getDeserializationContext());

    // no branch taken â†’ falls through
    assertNull(result);
}