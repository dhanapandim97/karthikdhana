import org.junit.jupiter.api.Test;

import java.util.Collections;

import static org.junit.jupiter.api.Assertions.*;

class YourClassTest {

    private final YourClass obj = new YourClass();

    @Test
    void testReturnsTrueWhenHttp404AndStatus1122() {
        Additionalstatus addStatus = new Additionalstatus(1122, null, null, null);
        Status status = new Status("404", null, Collections.singletonList(addStatus));

        boolean result = obj.isNoExistingIPI(status);

        assertTrue(result);
    }

    @Test
    void testReturnsFalseWhenHttpCodeNot404() {
        Additionalstatus addStatus = new Additionalstatus(1122, null, null, null);
        Status status = new Status("200", null, Collections.singletonList(addStatus));

        boolean result = obj.isNoExistingIPI(status);

        assertFalse(result);
    }

    @Test
    void testReturnsFalseWhenStatusCodeNot1122() {
        Additionalstatus addStatus = new Additionalstatus(9999, null, null, null);
        Status status = new Status("404", null, Collections.singletonList(addStatus));

        boolean result = obj.isNoExistingIPI(status);

        assertFalse(result);
    }

    @Test
    void testReturnsFalseWhenAdditionalStatusNull() {
        Status status = new Status("404", null, Collections.singletonList(null));

        boolean result = obj.isNoExistingIPI(status);

        assertFalse(result);
    }
}