@Test
void testNewDelegatingInstance() {
    JsonDeserializer<?> mockDelegate = mock(JsonDeserializer.class);
    JavaType mockType = TypeFactory.defaultInstance().constructType(String.class);

    IgnoreStringDeserializer deserializer =
            new IgnoreStringDeserializer(mockDelegate, mockType);

    JsonDeserializer<?> newOne = deserializer.newDelegatingInstance(mockDelegate);

    assertNotNull(newOne);
    assertTrue(newOne instanceof IgnoreStringDeserializer);
}